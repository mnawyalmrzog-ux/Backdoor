-- ÙˆØ¸ÙŠÙØ© Ø§Ù„ØªÙ†ÙÙŠØ°
local selectedRemote = nil
function execute(code)
    if selectedRemote and code ~= "" then
        if selectedRemote:IsA("RemoteEvent") then
            selectedRemote:FireServer(code)
        elseif selectedRemote:IsA("RemoteFunction") then
            selectedRemote:InvokeServer(code)
        end
    end
end

-- ÙˆØ¸ÙŠÙØ© Ø¥Ø¶Ø§ÙØ© Ø§Ù„Ø£Ø²Ø±Ø§Ø± Ø§Ù„ØªØ¯Ù…ÙŠØ±ÙŠØ©
function addCommand(name, script)
    local btn = Instance.new("TextButton", QuickActions)
    btn.Size = UDim2.new(1, 0, 0, 35)
    btn.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
    btn.Text = name
    btn.TextColor3 = Color3.white
    Instance.new("UICorner", btn)
    btn.MouseButton1Click:Connect(function() execute(script) end)
end

